<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.firstsolutions.Controller.TrackController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label style="-fx-font-size: 14; -fx-font-weight: bold;" text="Fridge Tracking" />

    <HBox spacing="10.0">
        <Label fx:id="usernameField" style="-fx-font-size: 18; -fx-font-weight: normal;" text="Name" underline="true" />
        <Region HBox.hgrow="ALWAYS" />
        <Button mnemonicParsing="false" onAction="#OnBackButtonClick" text="Back" />
    </HBox>

    <VBox alignment="CENTER" fillWidth="false" minHeight="200.0" minWidth="100.0" spacing="10.0">
        <TableView fx:id="trackTableView" prefHeight="374.0" prefWidth="478.0">
            <columns>
                <TableColumn fx:id="TrackID" prefWidth="100.0" text="TrackID" />
                <TableColumn fx:id="ItemID" prefWidth="100.0" text="ItemID" />
                <TableColumn fx:id="Quantity" prefWidth="100.0" text="Quantity" />
                <TableColumn fx:id="Message" prefWidth="100.0" text="Message" />
                <TableColumn fx:id="Name" prefWidth="100.0" text="Name" />
                <TableColumn fx:id="Role" prefWidth="100.0" text="Role" />
                <TableColumn fx:id="Username" prefWidth="100.0" text="Username" />
            </columns>
        </TableView>
    </VBox>
</VBox>
